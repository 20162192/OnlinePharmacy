<view class="swiper-box">
  <swiper indicator-dots="{{indicatorDots}}" indicator-color="#fff" indicator-active-color="#1E90FF"
    autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular="{{circular}}" class="swiper">
    <block wx:for="{{background}}" wx:key="*this">
      <swiper-item>
        <image src="{{item}}" class="img" />
      </swiper-item>
    </block>
  </swiper>
</view>